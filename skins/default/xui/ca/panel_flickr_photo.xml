<?xml version="1.0" encoding="utf-8"?>
<panel name="panel_flickr_photo">
	<combo_box name="resolution_combobox" tool_tip="Resolució de la imatge">
		<combo_box.item label="Finestra actual" name="CurrentWindow"/>
		<combo_box.item label="640x480" name="640x480"/>
		<combo_box.item label="800x600" name="800x600"/>
		<combo_box.item label="1024x768" name="1024x768"/>
	</combo_box>
		<combo_box name="filters_combobox" tool_tip="Filtres d'imatge">
		<combo_box.item label="Sense filtre" name="NoFilter"/>
	</combo_box>
	<check_box label="Mantén la relació d'aspecte" name="keep_aspect_ratio"/>
	<text name="working_lbl">
		Actualitzant...
	</text>
	<button label="Refresca" name="new_snapshot_btn" tool_tip="Fes clic per a refrescar"/>
	<button label="Previsualitza" name="big_preview_btn" tool_tip="Fes clic per a canviar la vista"/>
	<text name="title_label">
		Títol:
	</text>
	<text name="description_label">
		Descripció:
	</text>
	<check_box initial_value="true" label="Afegeix la ubicació de SL a la descripció" name="add_location_cb"/>
	<text name="tags_label">
		Etiquetes:
	</text>
	<text name="tags_help_label">
		Separa les etiquetes amb espais. Posa les etiquetes de diverses paraules entre cometes.
	</text>
	<combo_box name="rating_combobox" tool_tip="Qualificació del contingut a Flickr">
		<combo_box.item label="Qualificació Segura de Flickr" name="SafeRating"/>
		<combo_box.item label="Qualificació Moderada de Flickr" name="ModerateRating"/>
		<combo_box.item label="Qualificació Restringida de Flickr" name="RestrictedRating"/>
	</combo_box>
	<button label="Comparteix" name="post_photo_btn"/>
	<button label="Cancel·la" name="cancel_photo_btn"/>
</panel>
