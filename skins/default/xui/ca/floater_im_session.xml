<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="panel_im">
	<floater.string name="call_btn_start">
		Conv_toolbar_open_call
	</floater.string>
	<floater.string name="call_btn_stop">
		Conv_toolbar_hang_up
	</floater.string>
	<floater.string name="collapseline_icon" value="Conv_collapse_to_one_line"/>
	<floater.string name="expandline_icon" value="Conv_expand_one_line"/>
	<floater.string name="collapse_icon" value="Conv_toolbar_collapse"/>
	<floater.string name="expand_icon" value="Conv_toolbar_expand"/>
	<floater.string name="tear_off_icon" value="Conv_toolbar_arrow_ne"/>
	<floater.string name="return_icon" value="Conv_toolbar_arrow_sw"/>
	<floater.string name="participant_added" value="En/Na [NAME] s'ha unit a la conversa."/>
	<floater.string name="multiple_participants_added" value="S'ha convidat a [NAME] a la conversa."/>
	<floater.string name="tooltip_to_separate_window" value="Mou aquesta conversa a una finestra separada"/>
	<floater.string name="tooltip_to_main_window" value="Retorna aquesta conversa a la finestra principal"/>
	<floater.string name="start_call_button_tooltip" value="Inicia la connexió de veu"/>
	<floater.string name="end_call_button_tooltip" value="Tanca la connexió de veu"/>
	<floater.string name="expcol_button_not_tearoff_tooltip" value="Plega aquest panell"/>
	<floater.string name="expcol_button_tearoff_and_expanded_tooltip" value="Plega la llista de participants"/>
	<floater.string name="expcol_button_tearoff_and_collapsed_tooltip" value="Desplega la llista de participants"/>
	<view name="contents_view">
		<layout_stack name="main_stack">
			<layout_panel name="toolbar_panel">
				<menu_button name="view_options_btn" tool_tip="Opcions de visualització/ordenació"/>
				<menu_button name="gear_btn" tool_tip="Accions per al resident seleccionat"/>
				<button name="add_btn" tool_tip="Afegeix un participant a aquesta conversa"/>
				<button name="voice_call_btn" tool_tip="Inicia la connexió de veu"/>
				<button name="close_btn" tool_tip="Finalitza aquesta conversa"/>
				<button name="expand_collapse_btn" tool_tip="Desplega/plega aquest panell"/>
			</layout_panel>
			<layout_panel name="chat_layout_panel">
				<layout_stack name="input_panels">
					<layout_panel name="input_editor_layout_panel">
						<chat_editor label="A" name="chat_editor"/>
					</layout_panel>
					<layout_panel name="input_button_layout_panel">
						<button name="minz_btn" tool_tip="Mostra/amaga el panell de missatges"/>
					</layout_panel>
				</layout_stack>
			</layout_panel>
		</layout_stack>
	</view>
</floater>
