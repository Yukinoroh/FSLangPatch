<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="Joystick" title="Configuració del joystick">
	<floater.string name="JoystickDisabled">
		No
	</floater.string>
	<text name="joystick_lbl" width="60">
		Joystick:
	</text>
	<spinner label="Eix X" name="JoystickAxis1"/>
	<spinner label="Eix Y" name="JoystickAxis2"/>
	<spinner label="Eix Z" name="JoystickAxis0"/>
	<spinner label="EixCapcineig" name="JoystickAxis4"/> <!--"Eix de capcineig (pitch)"--> 
	<spinner label="EixGuinyada" name="JoystickAxis5"/>  <!--"Eix de guinyada (yaw)"--> 
	<spinner label="EixGuerxament" name="JoystickAxis3"/>  <!--Eix de guerxament (roll)"-->  
	<spinner label="Eix de zoom" name="JoystickAxis6"/>
	<check_box label="Zoom" name="ZoomDirect"/>
	<check_box label="Cursor 3D" name="Cursor3D" left="320" />
	<check_box label="Anivellament automàtic" name="Автовирівнювання"/>
	<text name="Режими керування:">
		Modes de control:
	</text>
	<check_box label="Avatar" name="JoystickAvatarEnabled"/>
	<check_box label="Crear" name="JoystickBuildEnabled"/>
	<check_box label="CàmeraVol" name="JoystickFlycamEnabled"/>
	<text name="joystick_monitor_label">
		Monitor del joystick
	</text>
	<icon name="button_light_0" tool_tip="Botó 0"/>
	<icon name="button_light_1" tool_tip="Botó 1"/>
	<icon name="button_light_2" tool_tip="Botó 2"/>
	<icon name="button_light_3" tool_tip="Botó 3"/>
	<icon name="button_light_4" tool_tip="Botó 4"/>
	<icon name="button_light_5" tool_tip="Botó 5"/>
	<icon name="button_light_6" tool_tip="Botó 6"/>
	<icon name="button_light_7" tool_tip="Botó 7"/>
	<icon name="button_light_8" tool_tip="Botó 8"/>
	<icon name="button_light_9" tool_tip="Botó 9"/>
	<icon name="button_light_10" tool_tip="Botó 10"/>
	<icon name="button_light_11" tool_tip="Botó 11"/>
	<icon name="button_light_12" tool_tip="Botó 12"/>
	<icon name="button_light_13" tool_tip="Botó 13"/>
	<icon name="button_light_14" tool_tip="Botó 14"/>
	<icon name="button_light_15" tool_tip="Botó 15"/>
	<scroll_container name="joystick_monitor_axes_scroller">
		<container_view name="joystick_monitor_axes_container">
			<stat_view name="axis_view_0">
				<stat_bar label="Eix 0" name="axis0"/>
			</stat_view>
			<stat_view name="axis_view_1">
				<stat_bar label="Eix 1" name="axis1"/>
			</stat_view>
			<stat_view name="axis_view_2">
				<stat_bar label="Eix 2" name="axis2"/>
			</stat_view>
			<stat_view name="axis_view_3">
				<stat_bar label="Eix 3" name="axis3"/>
			</stat_view>
			<stat_view name="axis_view_4">
				<stat_bar label="Eix 4" name="axis4"/>
			</stat_view>
			<stat_view name="axis_view_5">
				<stat_bar label="Eix 5" name="axis5"/>
			</stat_view>
			<stat_view name="axis_view_6">
				<stat_bar label="Eix 6" name="axis6"/>
			</stat_view>
			<stat_view name="axis_view_7">
				<stat_bar label="Eix 7" name="axis7"/>
			</stat_view>
		</container_view>
	</scroll_container>
	<text name="XScale">
		Escala X
	</text>
	<text name="YScale">
		Escala Y
	</text>
	<text name="ZScale">
		Escala Z
	</text>
	<text name="PitchScale">
		Escala Capcineig
	</text>
	<text name="YawScale">
		Escala de Guinyada
	</text>
	<text name="RollScale">
		Escala Guerxament
	</text>
	<text name="XDeadZone">
		Zona morta X
	</text>
	<text name="YDeadZone">
		Zona morta Y
	</text>
	<text name="ZDeadZone">
		Zona morta Z
	</text>
	<text name="PitchDeadZone">
		ZonaMortaCapcineig
	</text>
	<text name="YawDeadZone">
		ZonaMortaGuinyada
	</text>
	<text name="RollDeadZone" width="160" >
		ZonaMortaGuerxament
	</text>
	<text name="Feathering">
		Suavitzat:
	</text>
	<text name="ZoomScale2">
		Escala de Zoom
	</text>
	<text name="ZoomDeadZone">
		Zona morta de Zoom
	</text>
	<button label="Valors per defecte per a SpaceNavigator" name="SpaceNavigatorDefaults" width="280" />
	<button label="D'acord" label_selected="D'acord" name="ok_btn"/>
	<button label="Cancel·la" label_selected="Cancel·la" name="cancel_btn"/>
